{
  "name": "ndi_biometric_service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rm -r dist",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:pm2": "NODE_ENV=development pm2 start dist/src/main.js --name biometric-service --update-env",
    "start:dev": "env-cmd -f .env nest start --watch",
    "start:debug": "env-cmd -f .env nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "start:docker": "NATS_URL=nats://host.docker.internal:4222 yarn start",
    "docker:build": "docker build -t biometric-service .",
    "docker:run": "docker run --name biometric-service -dp 3001:3001 --env-file ./.env biometric-service",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.441.0",
    "@elastic/ecs-winston-format": "^1.3.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/class-transformer": "^0.4.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^9.2.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/terminus": "^9.0.0",
    "@regulaforensics/facesdk-webclient": "^6.2.0",
    "@types/pg": "^8.11.6",
    "async-retry": "^1.3.3",
    "cache-manager": "^5.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^17.2.0",
    "face-api.js": "^0.22.2",
    "helmet": "^7.1.0",
    "jimp": "^0.16.2",
    "joi": "^17.6.0",
    "jwks-rsa": "^3.0.1",
    "liquibase": "^4.4.0",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "nats": "^2.6.0",
    "nestjs-cls": "^3.4.0",
    "nestjs-form-data": "^1.8.3",
    "passport-jwt": "^4.0.1",
    "pg": "^8.7.3",
    "pixelmatch": "^5.3.0",
    "pngjs": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41",
    "winston": "3.4",
    "winston-elasticsearch": "0.16.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^9.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.0",
    "@types/pixelmatch": "^5.2.4",
    "@types/supertest": "^2.0.11",
    "@types/url-join": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^4.1.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.0",
    "jest": "^27.2.5",
    "jwks-rsa": "^3.0.1",
    "lint-staged": "^13.2.2",
    "passport": "^0.6.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "*"
  },
  "lint-staged": {
    "src/**/*.{ts}": "prettier --write",
    "src/**/*.ts": "eslint"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
